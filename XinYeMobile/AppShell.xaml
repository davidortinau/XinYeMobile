<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="XinYeMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:mi="http://www.aathifmahir.com/dotnet/2022/maui/icons"
    xmlns:local="clr-namespace:XinYeMobile.Pages"
    Shell.FlyoutBehavior="Disabled"
    Shell.TabBarUnselectedColor="Gray"
    Shell.TabBarForegroundColor="{StaticResource Primary}"
    Shell.TabBarTitleColor="{StaticResource Primary}"
    Shell.ForegroundColor="{AppThemeBinding Dark={StaticResource White},Light={StaticResource Black}}"
    Title="XinYeMobile">
    <TabBar>
        <ShellContent
                        Title="首页"
                        ContentTemplate="{DataTemplate local:HomePage}"
                        Icon="{mi:Material Icon=Home}"
                        Route="HomePage" />

    </TabBar>


</Shell>
